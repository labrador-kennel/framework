{
  "name": "labrador-kennel/http",
  "description": "A web framework for building applications with Amp and Annotated Container.",
  "license": "MIT",
  "scripts": {
    "code-lint": "vendor/bin/labrador-cs src/ test/",
    "test": "vendor/bin/phpunit",
    "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html ./build/coverage"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/cspray/http-server-session"
    }
  ],
  "minimum-stability": "beta",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "amphp/http": "^v2.0",
    "amphp/http-server": "^v3.x-dev",
    "amphp/http-server-session": "dev-bug/http-v2-upgrade",
    "amphp/log": "^v2.0",
    "cspray/annotated-container": "^v2.0",
    "cuyz/valinor": "^1.2",
    "labrador-kennel/async-event": "^3.0",
    "labrador-kennel/async-event-autowire":  "dev-main",
    "league/uri": "^6.7",
    "league/uri-components": "^2.4",
    "monolog/monolog": "^3",
    "nikic/fast-route": "^1.3",
    "ocramius/package-versions": "^2.7",
    "php-di/php-di": "^v7.0",
    "ramsey/uuid": "^4.4"
  },
  "require-dev": {
    "amphp/http-client": "^v5.0",
    "amphp/phpunit-util": "^v3.0",
    "cspray/labrador-coding-standard": "0.1.0",
    "cspray/stream-buffer-intercept": "^0.1.0",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5",
    "psalm/phar": "^5.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Labrador\\Http\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Labrador\\Http\\Test\\": "test",
      "Labrador\\HttpDummyApp\\": "dummy_app"
    }
  },
  "extra": {
    "$annotatedContainer": {
      "initializers": [
        "Labrador\\Http\\Autowire\\Initializer"
      ]
    }
  }
}
